
@{
    ViewBag.Title = "Resultados";
}

<head>
    <title>Table V04</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/perfect-scrollbar/perfect-scrollbar.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/css/table/util.css">
    <link rel="stylesheet" type="text/css" href="~/css/table/main.css">
    <!--===============================================================================================-->

</head>
<body>

    <div>
        <div class="container-table100">
            <div class="wrap-table100">


                <div class="table100 ver3 m-b-110">
                    <div class="table100-head">
                        <table>
                            <thead>
                                <tr class="row100 head">
                                    <th class="cell100 column1">N°</th>
                                    <th class="cell100 column2">Fecha Giro</th>
                                    <th class="cell100 column3">Total Facturado</th>-
                                    <th class="cell100 column4">Fecha Vencimiento</th>
                                    <th class="cell100 column5">TE%</th>
                                    <th class="cell100 column6">D%</th>
                                    <th class="cell100 column7">Descuento</th>
                                    <th class="cell100 column8">Costo Inicial</th>
                                    <th class="cell100 column9">Costo Final</th>-
                                    <th class="cell100 column10">Valor Neto</th>
                                    <th class="cell100 column11">Valor Recibio</th>
                                    <th class="cell100 column12">Valor Entregado</th>
                                    <th class="cell100 column13">TCEA%</th>

                                </tr>
                            </thead>
                        </table>
                    </div>

                    <div class="table100-body js-pscroll">
                        <table>
                            <tbody class="form-mid-w3ls"></tbody>
                        </table>
                    </div>

                    @using (Html.BeginForm("Resultados", "Home"))
                    {
                        <input type="submit" value="Resultados" />
                    }


                </div>



            </div>
        </div>
    </div>


    <!--===============================================================================================-->
    <script src="~/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/bootstrap/js/popper.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script>
		$('.js-pscroll').each(function(){
			var ps = new PerfectScrollbar(this);

			$(window).on('resize', function(){
				ps.update();
			})
		});


    </script>
    <!--===============================================================================================-->
    <script src="~/js/main.js"></script>

</body>

